{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ecommerce": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/ecommerce",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "src/assets/images",
                "output": "/assets/images"
              },
              {
                "glob": "**/*",
                "input": "src/assets/fonts",
                "output": "/assets/fonts"
              },
              {
                "glob": "**/*.ttf",
                "input": "src/assets/fonts",
                "output": "/assets/fonts"
              },
              {
                "glob": "**/*.svg",
                "input": "src/assets/images",
                "output": "/assets/images"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/style.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/elegant-icons.css",
              "src/assets/css/jquery-ui.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/slicknav.min.css",
              "node_modules/owl.carousel/dist/assets/owl.carousel.css",
              "node_modules/owl.carousel/dist/assets/owl.theme.default.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/fonts/font-awesome-4.7.0/css/font-awesome.min.css",
              "src/assets/fonts/Linearicons-Free-v1.0.0/icon-font.min.css",
              "src/assets/vendor/animate/animate.css",
              "src/assets/vendor/css-hamburgers/hamburgers.min.css",
              "src/assets/vendor/animsition/css/animsition.min.css",
              "src/assets/vendor/select2/select2.min.css",
              "src/assets/vendor/daterangepicker/daterangepicker.css",
              "src/assets/css/util.css",
              "src/assets/css/soft-ui-dashboard.min.css",
              "src/assets/css/soft-ui-dashboard.css",
              "src/assets/css/nucleo-svg.css",
              "src/assets/css/nucleo-icons.css",

              "src/styles.css"
              
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",  
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/js/jquery.countdown.min.js",
              "src/assets/js/jquery.slicknav.js",
              "src/assets/js/jquery.nicescroll.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/jquery-ui.min.js",
              "src/assets/js/mixitup.min.js",
              "node_modules/owl.carousel/dist/owl.carousel.min.js", 
              "src/assets/vendor/jquery/jquery-3.2.1.min.js",
              "src/assets/vendor/animsition/js/animsition.min.js",
              "src/assets/vendor/bootstrap/js/popper.js",
              "src/assets/vendor/bootstrap/js/bootstrap.min.js",
              "src/assets/vendor/daterangepicker/moment.min.js",
              "src/assets/vendor/daterangepicker/daterangepicker.js",
              "src/assets/vendor/countdowntime/countdowntime.js",
              "src/assets/js/soft-ui-dashboard.min.js",
              "src/assets/js/soft-ui-dashboard.js",
              "src/assets/js/plugins/bootstrap-notify.js",
              "src/assets/js/plugins/perfect-scrollbar.min.js",
              "src/assets/js/plugins/smooth-scrollbar.min.js",
              "src/assets/js/plugins/Chart.extension.js",
              "src/assets/js/plugins/chartjs.min.js",
              "src/assets/js/core/bootstrap.bundle.min.js",
              "src/assets/js/core/bootstrap.min.js",
              "src/assets/js/core/popper.min.js",
              "src/assets/js/main.js"
            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ecommerce:build:production"
            },
            "development": {
              "buildTarget": "ecommerce:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/style.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/elegant-icons.css",
              "src/assets/css/jquery-ui.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/slicknav.min.css",
              "node_modules/owl.carousel/dist/assets/owl.carousel.css",
              "node_modules/owl.carousel/dist/assets/owl.theme.default.css",
              "src/assets/css/soft-ui-dashboard.min.css",
              "src/assets/css/soft-ui-dashboard.css",
              "src/assets/css/nucleo-svg.css",
              "src/assets/css/nucleo-icons.css",
             
              "src/styles.css"
              
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/js/jquery.countdown.min.js",
              "src/assets/js/jquery.slicknav.js",
              "src/assets/js/jquery.nicescroll.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/jquery-ui.min.js",
              "src/assets/js/mixitup.min.js",
              "node_modules/owl.carousel/dist/owl.carousel.min.js",
              "src/assets/vendor/jquery/jquery-3.2.1.min.js",
              "src/assets/vendor/animsition/js/animsition.min.js",
              "src/assets/vendor/bootstrap/js/popper.js",
              "src/assets/vendor/bootstrap/js/bootstrap.min.js",
              "src/assets/vendor/daterangepicker/moment.min.js",
              "src/assets/vendor/daterangepicker/daterangepicker.js",
              "src/assets/vendor/countdowntime/countdowntime.js",
              "src/assets/js/soft-ui-dashboard.min.js",
              "src/assets/js/soft-ui-dashboard.js",
              "src/assets/js/plugins/bootstrap-notify.js",
              "src/assets/js/plugins/perfect-scrollbar.min.js",
              "src/assets/js/plugins/smooth-scrollbar.min.js",
              "src/assets/js/plugins/Chart.extension.js",
              "src/assets/js/plugins/chartjs.min.js",
              "src/assets/js/core/bootstrap.bundle.min.js",
              "src/assets/js/core/bootstrap.min.js",
              "src/assets/js/core/popper.min.js",
              
              "src/assets/js/main.js"
            ]
          }
        }
      }
    }
  }
}
